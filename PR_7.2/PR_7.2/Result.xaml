<Window x:Class="PR_7._2.Result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Результаты" Height="200" Width="300"
        ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="AreaTextBlock" FontSize="16" Margin="10" />
        <TextBlock x:Name="MiddleLineTextBlock" FontSize="16" Margin="10,30,10,0"/>
        <TextBlock x:Name="DiagonalTextBlock" FontSize="16" Margin="10,49,10,0"/>
        <Button Content="OK" Width="80" Height="30" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,10,0,10" Click="Ok_Click"/>
    </Grid>
</Window>
